<%- include("partials/header.ejs") %>

<div class="Notificationcontaner">
    <% if (notifications && notifications.length > 0) { %>


    <% notifications.forEach(notification => { %>
        <div class="notification">
        <div class="notificationTitle"><a href="/notification-Details/<%=notification.id%>"><%= notification.title %></a></div>
        <div class="notificationContent"><%= notification.content.substring(0, 100) %></div>

        <div class="notificationDate"><%= notification.date %></div>
    </div>
    <% }); %>

       

<% } else { %>
    <p>No notifications available</p>
<% } %>

</div>

<%- include("partials/footer.ejs") %>
