    footer p {
      width: 100vw;
      text-align: center;
      font-size: 1.3rem;
    }

    footer svg {
      height: 3.5vh;
    }

    footer svg:hover {
      height: 3.55vh;
      fill: rgb(251, 118, 118);
    }

    /* Scrollbar style */
    ::-webkit-scrollbar {
      display: none;
    }

    /* Track */
    ::-webkit-scrollbar-track {
      background: transparent;
    }

    /* Handle */
    ::-webkit-scrollbar-thumb {
      background: black;
    }

    /* Handle on hover */
    ::-webkit-scrollbar-thumb:hover {
      background: rgb(96, 0, 0);
    }

    .container {
      opacity: 0;
      transition: opacity 3s ease;
      height: 100vh;
    }

    .donate-container {
      /* background: grey; */
      padding: 20px 0;
      width: 80vw;
      margin: auto;
      display: flex;
      flex-direction: row;
      flex-wrap: wrap;
      align-items: center;
      justify-content: space-between;
      background-repeat: no-repeat;
    }

    .donate-container h1 {
      width: 100vw;
      text-align: center;
    }

    .qr-image {
      /* background: #000; */
      height: 80vh;
      width: 35vw;
    }

    .donate-form {
      /* background: blueviolet; */
      height: 80vh;
      width: 35vw;
      display: flex;
      flex-direction: column;
      align-items: center;
      justify-content: center;
    }

    .donate-form form {
      margin-top: 20px;
    }

    .donate-form h2 {
      text-decoration: none;
    }

    .donate-form p {
      margin-top: 20px;
      background: white;
    }

    .donate-form button {
      background: green;
    }

    .qr-form button {
      width: 200px;
    }


    .donate-form button:hover {
      background: rgb(83, 187, 83);
      color: #7b7a7a;

    }



    .loader {
      height: 65px;
      width: 65px;
      display: flex;
      align-items: center;
      justify-content: center;
      position: fixed;
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%);
      z-index: 1000;
    }

    .spinner {
      height: 100%;
      width: 100%;
      background: transparent;
      border: 5px solid transparent;
      border-top: 5px solid red;
      border-radius: 50%;
      animation: spin 1.5s linear infinite;
      position: absolute;
      opacity: 0.7;
    }

    .loader img {
      border-radius: 50%;
      height: 100%;
      width: 100%;
      z-index: 1001;
      opacity: 1.5;
    }

    @keyframes spin {
      0% {
        transform: rotate(0deg);
      }

      100% {
        transform: rotate(360deg);
      }
    }





